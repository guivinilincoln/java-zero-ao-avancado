package entities;

public class Triangle {
	
	public double a;
	public double b;
	public double c;
	
	public Triangle(double a, double b, double c) {
		
		this.a = a;
		this.b = b;
		this.c = c; 
		
	}
	
	public double calcularArea() {
		double p = (xA + xB + xC) / 2.0;
		double areaX = Math.sqrt(p * (p - xA) * (p - xB) * (p - xC));
	}
	
	
}
