package entities;

public class Product {
	
	private String name;
	private double price;
	private int quantity;
	
	public double TotalValueInStock() {
		return  this.price * this.quantity;
	}
	
	public void addProducts(int qtd) {
		this.quantity += qtd;
	}
	
	public void removeProducts(int qtd) {
		
	}
	
}
